{
  "verbose": true,
  "dp_cgans": {
    "discriminator_steps": 1,
    "epochs": 1,
    "batch_size": 100,
    "gen_size": 100,
    "pac": 10,
    "embedding_dim": 128,
    "generator_dim": [128, 128, 128],
    "discriminator_dim": [128, 128, 128],
    "files": {
      "directory": "../experiments/onto_cgans/data/%s",
      "input": "patient_data/seen.csv",
      "output": "generated.csv"
    },
    "results": {
      "directory": "results",
      "file": "results.csv"
    },
    "field_names": null,
    "field_types": null,
    "field_transformers": null,
    "anonymize_fields": null,
    "primary_key": null,
    "constraints": null,
    "table_metadata": null,
    "generator_lr": 1e-4,
    "generator_decay": 1e-6,
    "discriminator_lr": 1e-4,
    "discriminator_decay": 1e-6,
    "log_frequency": true,
    "cuda": true,
    "rounding": "auto",
    "min_value": "auto",
    "max_value": "auto",
    "private": false
  },
  "embedding": {
    "files": {
      "directory": "embedding"
    },
    "model": {
      "load": false,
      "save": true,
      "use_root": true,
      "directory": "model",
      "file": "model.embedding",
      "save_key_vectors": true
    },
    "datasets": {
      "use_root": false,
      "directory": "../experiments/preprocessing/data/%s/merged",
      "training": "merged.rdf",
      "testing": null,
      "validation": null
    },
    "projection": {
      "type": "owl2vecstar",
      "bidirectional_taxonomy": false,
      "include_literals": false,
      "only_taxonomy": false,
      "use_taxonomy": false,
      "relations": null
    },
    "random_walks": {
      "type": "deepwalk",
      "num_walks": 15,
      "walk_length": 6,
      "workers": 4,
      "alpha": 0.01,
      "p": 10,
      "q": 0.1,
      "outfile": {
        "use_root": true,
        "directory": "random_walks",
        "file": "out.txt"
      }
    },
    "word2vec": {
      "epochs": 15,
      "dimensions": 25,
      "window": 4,
      "min_count": 3
    },
    "references": [
      "../experiments/preprocessing/data/%s/dictionaries/HPO.dict",
      "../experiments/preprocessing/data/%s/dictionaries/ORDO.dict"
    ]
  },
  "zsl": {
    "enabled": true,
    "files": {
      "directory": "../experiments/ontozsl_cgans/data/%s"
    },
    "class_header": "rare_disease",
    "model": {
      "name": "bert-base-uncased",
      "threshold": 0.75,
      "unknown_class": "unknown",
      "load": false,
      "save": true,
      "prompt": false,
      "files": {
        "use_root": true,
        "directory": "model",
        "model": "ontozsl_cgan.model",
        "results": "results",
        "output": "out.csv"
      }
    }
  }
}